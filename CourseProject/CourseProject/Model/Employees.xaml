<Window x:Class="CourseProject.Model.Employees"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CourseProject.Model"
        mc:Ignorable="d"
        Title="Employees" Height="450" Width="800">
    <Grid>
        <Button Content="Назад" HorizontalAlignment="Left" Height="54" VerticalAlignment="Top" Width="94"/>
        <DataGrid HorizontalAlignment="Left" Height="432" Margin="95,0,0,0" VerticalAlignment="Top" Width="706" d:ItemsSource="{d:SampleData ItemCount=7}" RenderTransformOrigin="0.5,0.5">
            <DataGrid.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform AngleX="0.38"/>
                    <RotateTransform/>
                    <TranslateTransform X="0.627"/>
                </TransformGroup>
            </DataGrid.RenderTransform>
        </DataGrid>

    </Grid>
</Window>
